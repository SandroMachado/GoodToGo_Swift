git status && git add . && git commit -m "Lorem epsilon" && git push origin master
git status && git add . && git commit -m "Marvel challenge - Comics pagination" && git push origin master

/*
 *  Remove Git
 */
defaults write com.apple.finder AppleShowAllFiles TRUE
killall Finder
# Delete Git folder

/*
 *  Add Git
 */
git init
git add .
git commit -m "First commit"
git remote rm origin
git remote add origin https://github.com/ricardopsantos/GoodToGo_Swift
git push origin master
git push -f origin master.


/*
 *  FORCE!
 */
# http://stackoverflow.com/questions/5509543/how-do-i-properly-force-a-git-push/12610763#12610763
git push origin master --force
